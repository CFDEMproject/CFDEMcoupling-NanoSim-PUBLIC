{
   "mainSettings":
   {
        "doFiltering": true,
        "doSampling":  true,
        "doBinning":   true,
        "dumpFormat": "json",   
        "FieldsToRegister":
        {
            "Vectorfields": "solids-x-velocity air-x-velocity udm-x",
            "Scalarfields": "solids-vof" 
        },
         "filterBCs":
        {
        "x":"periodic",
        "y":"periodic",
        "z":"periodic"
        },
        "verbose": false,
        "storageWriteFields"    :  false,
        "storageWriteParticles" :  false,
        "interfaceWriteFields" :   true
   },
  
   "myFilter0":	  { 
                     "CoordSys" :  0, 
                     "x"        :  0.00499, 
                     "y"        :  0.00499,
                     "z"        :  0.00499
                  },
   "myFilter1":	  { 
                     "CoordSys" :  0, 
                     "x"        :  0.050, 
                     "y"        :  0.050,
                     "z"        :  0.050
                  },
   "myFilter2":	  { 
                     "CoordSys" :  0, 
                     "x"        :  0.400, 
                     "y"        :  0.400,
                     "z"        :  0.400
                  },
   "filtFavre": 
            {
             "type":"Favre",
             "VectorfieldsToFilter" : "solids-x-velocity",
             "ScalarfieldsToFilter" : " ",
             "phaseFractionField"   : "solids-vof",
             "lagrangian"           : false,
             "invertPhaseFraction"  : false
            },
   "filtFavreGas": 
            {
             "type":"Favre",
             "VectorfieldsToFilter" : "air-x-velocity",
             "ScalarfieldsToFilter" : " ",
             "phaseFractionField"   : "solids-vof",
             "lagrangian"           : false,
             "invertPhaseFraction"  : true
            },   

   "filtAlg": 
            {
             "type":"Algebraic",
             "VectorfieldsToFilter": "udm-x",
             "ScalarfieldsToFilter": "solids-vof",
             "lagrangian": false
            },
             
   "filtSlipY": {
                  "type"            : "general",
                  "marker"          : "solids-vof_filtAlg",
                  "VFieldsToSample" : "air-x-velocity_filtFavreGas  solids-x-velocity_filtFavre",
                  "component"       : 1,
                  "Formula"         : " vec1 - vec2 ",
                  "saveOnlyFormula" : true,
                  "sampleCount"     : -1,
                  "save2Disk"       : true,
                  "save2Bin"        : true,
                  "lagrangian"      : false,
                  "overwrite"       : false                        
               },
   
   "binFiltSlipY": {
                  "bincount"        : 30,
                  "binUpBorder"     : 0.65,
                  "binLowBorder"    : 0,
                  "overwrite"       : true
           },

             
   "filtBetaY": {
                  "type"            : "general",
                  "marker"          : "solids-vof_filtAlg",
                  "VFieldsToSample" : "udm-x_filtAlg  air-x-velocity_filtFavreGas  solids-x-velocity_filtFavre",
                  "component"       : 1,
                  "Formula"         : "( vec1 ) % (vec2 - vec3)",
                  "saveOnlyFormula" : true,
                  "sampleCount"     : -1,
                  "save2Disk"       : true,
                  "save2Bin"        : true,
                  "lagrangian"      : false,
                  "overwrite"       : false
               },
   
   "binFiltBetaY": {
                  "bincount"        : 30,
                  "binUpBorder"     : 0.65,
                  "binLowBorder"    : 0,
                  "overwrite"       : true
           }

}    
